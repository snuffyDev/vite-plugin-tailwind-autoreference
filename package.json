{
  "name": "vite-plugin-tailwind-autoreference",
  "version": "0.1.0",
  "description": "Automatically `@reference` your Tailwind CSS file in Svelte, Vue & other SFC files.",
  "author": "snuffyDev",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "release": "release-it",
    "lint": "eslint . --ext .ts,.js,.svelte",
    "format": "prettier --write ."
  },
  "keywords": [
    "autoreference",
    "reference",
    "sfc",
    "svelte",
    "sveltekit",
    "tailwind",
    "tailwindcss",
    "vite",
    "vite-plugin",
    "vue"
  ],
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "release-it": "^18.1.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "vite": "^6.2.6"
  }
}
